{
    "name": "ShanBay Extension",
    "action": {
        "default_title": "ShanBay Extension",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "description": "收集单词至扇贝库",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "bookmarks"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "key": ""
}